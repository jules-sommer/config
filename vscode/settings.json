{
  "editor.tabSize": 2,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.indentation": false,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.bracketPairs": false, // advanced indent guides (But only for brackets) (This does not turn off editor.bracketPairColorization)
  "editor.guides.bracketPairsHorizontal": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.experimentalWhitespaceRendering": "font",
  //         _                 _   _                      _                 
  //  __   _(_)___ _   _  __ _| | | |___      _____  __ _| | _____ _ __ ___ 
  //  \ \ / / / __| | | |/ _` | | | __\ \ /\ / / _ \/ _` | |/ / _ \ '__/ __|
