    "editorBracketHighlight.foreground2": "#FFD085",
    "editorBracketHighlight.foreground3": "#FFF085",
    "editorBracketHighlight.foreground4": "#D0FF85",
    "editorBracketHighlight.foreground5": "#85FFD0",
    "editorBracketHighlight.foreground6": "#858AFF",
    "editorBracketPairGuide.activeBackground1": "#FF85A2",
    "editorBracketPairGuide.activeBackground2": "#FFD085",
    "editorBracketPairGuide.activeBackground3": "#FFF085",
    "editorBracketPairGuide.activeBackground4": "#D0FF85",
    "editorBracketPairGuide.activeBackground5": "#85FFD0",
    "editorBracketPairGuide.activeBackground6": "#858AFF",
    "statusBar.debuggingBackground": "#000000",
    "statusBar.debuggingBorder": "#531643",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBar.foreground": "#ff3ff2b1",
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f"
  },
  // Indent transparent depth-based infill
  "indentColorizer.colors": [
    // Soft Pink
    "rgba(255, 138, 216, 0.05)",
    // Light Orange
    "rgba(255, 165, 130, 0.075)",
    // Pastel Yellow
    "rgba(255, 229, 153, 0.090)",
    // Mint Green
    "rgba(162, 255, 178, 0.1)",
    // Sky Blue
    "rgba(130, 207, 255, 0.125)",
    // Lavender
    "rgba(178, 156, 255, 0.15)",
    // Pale Purple
    "rgba(204, 153, 255, 0.2)",
    // Soft Magenta
    "rgba(255, 153, 204, 0.25)"
  ],
  "indentRainbow.errorColor": "rgba(128,32,32,0.6)",
  "indentColorizer.colorOnWhiteSpaceOnly": true, // false is the default
  "indentColorizer.ignoreLinePatterns": [
    "/[ \t]* [*]/g", // lines begining with <whitespace><space>*
    "/[ \t]+[/]{2}/g" // lines begininning with <whitespace>//
  ],
  "indentRainbow.excludedLanguages": [
    "plaintext",
    "json"
  ],
  "indentRainbow.colors": [
    // Soft Pink
    "rgba(255, 138, 216, 0.1)",
    // Light Orange
    "rgba(255, 165, 130, 0.11)",
    // Pastel Yellow
    "rgba(255, 229, 153, 0.12)",
    // Mint Green
    "rgba(162, 255, 178, 0.13)",
    // Sky Blue
    "rgba(130, 207, 255, 0.15)",
    // Lavender
    "rgba(178, 156, 255, 0.2)",
    // Pale Purple
    "rgba(204, 153, 255, 0.3)",
    // Soft Magenta
    "rgba(255, 153, 204, 0.35)"
  ],
  // 
  "blockman.n04Sub01ColorComboPresetForDarkTheme": "Classic Dark 1 (Super gradients)",
  // "blockman.n04Sub02ColorComboPresetForLightTheme": "Classic Light (Super gradients)",
  // "blockman.n04Sub03ColorComboPresetForHighContrastTheme": "Classic Dark 1 (Super gradients)",
  "blockman.n04Sub05MinDistanceBetweenRightSideEdges": 4,
  "blockman.n04Sub06AdditionalPaddingRight": 2,
  "blockman.n17CustomColorOfFocusedBlock": "same",
  "blockman.n23AnalyzeSquareBrackets": true,
  "blockman.n24AnalyzeRoundBrackets": true,
  // 
  "blockman.n33A01B1FromDepth0ToInwardForAllBorders": "10,0,0,1; empty",
  "blockman.n33A02B1FromFocusToOutwardForAllBorders": "!",
  "blockman.n33A03B1FromDepth0ToInwardForFocusTreeBorders": "!10,0,0,1; empty",
  "blockman.n33A03B2FromDepth0ToInwardForFocusTreeBackgrounds": "!",
  "blockman.n33A04B1FromFocusToOutwardForFocusTreeBorders": "!40,0,0,0; basic > basic",
  "blockman.n20CustomColorOfDepth0Border": "rgba(255,255,255,0.1)",
  "blockman.n33A04B2FromFocusToOutwardForFocusTreeBackgrounds": "40,0,0,0; basic > basic",
  "blockman.n33A05B1FromFocusToInwardForAllBorders": "50,0,0,0; basic > basic",
  "indentColorizer.errorColor": "",
  "indentColorizer.tabmixColor": "rgba(128,32,96,0.6)",
