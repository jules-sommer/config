  // BLOCKMAN - Bracket block colorizer
  "blockman.n28TimeToWaitBeforeRerenderAfterLastChangeEvent": 0.2,
  "blockman.n16EnableFocus": true,
  "blockman.n04ColorComboPreset": "Classic Dark 1 (Super gradients)",
  "blockman.n03MaxDepth": 15,
  "blockman.n21BorderRadius": 10,
  "blockman.n35A01DisableRecommendationDialog": true,
  "blockman.n27B01HideBlockOfFocusedSingleLineArea": false,
  "blockman.n34A01BorderThicknessOfNonFocusedBlock": 0.5,
  "blockman.n34A02BorderThicknessOfFocusedBlock": 1,
  "blockman.n19CustomColorOfBlockBorder": "#17000d",
  "blockman.n22CustomColorOfBlockBackground": "#000",
  "blockman.n18CustomColorOfFocusedBlockBorder": "#ff00ff",
  // 
  // 
  "tailwindCSS.experimental.classRegex": [
    [
      "cva\\(([^)]*)\\)",
      "[\"'`]([^\"'`]*).*?[\"'`]"
    ],
    [
      "cx\\(([^)]*)\\)",
      "(?:'|\"|`)([^']*)(?:'|\"|`)"
    ],
    [
      "cn\\(([^)]*)\\)",
      "(?:'|\"|`)([^']*)(?:'|\"|`)"
    ],
    [
      "clsx\\(([^)]*)\\)",
      "(?:'|\"|`)([^']*)(?:'|\"|`)"
    ]
  ],
  "better-comments.highlightPlainText": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "[NOTE]:",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": true,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "TODO:",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "quokka.compactMessageOutput": true,
  "quokka.automaticRestart": true,
  "github.copilot.editor.enableCodeActions": true,
  "github.copilot.advanced": {
    "indentationMode": {
      "python": "clientandserver",
      "javascript": "clientandserver",
      "javascriptreact": "clientandserver",
      "jsx": "clientandserver",
      "typescript": "clientandserver",
      "typescriptreact": "clientandserver",
      "go": "clientandserver",
      "ruby": "clientandserver",
      "*": "clientandserver"
    },
  },
  "console-ninja.featureSet": "Community",
  "rust-analyzer.checkOnSave": true,
  "rust-analyzer.cargo.buildScripts.rebuildOnSave": true,
  "rust-analyzer.procMacro.attributes.enable": true,
  "rust-analyzer.diagnostics.disabled": [
    "unresolved-proc-macro",
  ],
  "rust-analyzer.cargo.sysroot": "discover",
  "rust-analyzer.trace.extension": true,
  "rust-analyzer.check.features": "all",
  "rust-analyzer.inlayHints.typeHints.enable": true,
  "github.copilot.enable": {
    "markdown": false,
    "scminput": false
  },
  "nix.enableLanguageServer": true,
  "nix.formatterPath": "nixfmt",
  "terminal.integrated.env.linux": {},
  "terminal.integrated.allowChords": false,
