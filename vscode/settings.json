  "editor.fontFamily": "JetBrains Mono",
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "editor.fontSize": 17,
  "terminal.integrated.fontSize": 17,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.autoClosingBrackets": "languageDefined",
  "editor.autoClosingOvertype": "auto",
  "editor.fontLigatures": true,
  "editor.inlayHints.enabled": "on", // new API for indent guides. The old one is: "editor.renderIndentGuides": false,
  "editor.wordWrap": "off",
  "diffEditor.wordWrap": "off",
  "workbench.colorTheme": "Dark+ Black",
  "workbench.commandPalette.experimental.askChatLocation": "quickChat",
  "editor.detectIndentation": false,
  "editor.autoIndent": "advanced",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.linkedEditing": true,
  "editor.hover.delay": 135,
  "editor.emptySelectionClipboard": true,
  "editor.multiCursorPaste": "full",
  "editor.pasteAs.showPasteSelector": "afterPaste",
  "editor.experimental.asyncTokenization": true,
  "editor.lineHeight": 1.475,
  "workbench.sideBar.location": "right",
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.organizeImports": "always"
  },
  "editor.indentSize": 2,
  "editor.insertSpaces": true,
  /* Terminal Appearance and Behaviour */
  "color-highlight.markRuler": false,
  "workbench.tree.renderIndentGuides": "none",
  // Cursor
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "block",
  "editor.cursorSurroundingLines": 3,
  "editor.cursorWidth": 2.5,
  "terminal.integrated.cursorStyle": "block",
  "editor.multiCursorModifier": "ctrlCmd",
  // 
  "editor.renderLineHighlight": "none",
  "editor.glyphMargin": false,
  "editor.lineNumbers": "interval",
  "editor.showFoldingControls": "never",
  "workbench.layoutControl.type": "menu",
  "window.title": "${dirty}${activeEditorShort}",
  "window.titleBarStyle": "custom",
  "breadcrumbs.enabled": false,
  // 
  "workbench.colorCustomizations": {
    "editorGutter.background": "#000000",
    "editorGroupHeader.tabsBackground": "#000000",
    "tab.inactiveBackground": "#000000",
    "breadcrumb.background": "#000000",
    "terminal.background": "#000000",
    "terminal.foreground": "#ffffff",
    "editor.background": "#000000",
    // terminal menu bar background color
    "menu.background": "#000000",
    "editorMarkerNavigation.background": "#ff0000",
    "titleBar.activeBackground": "#000000",
    "panelTitle.activeBorder": "#ff0000",
    "tab.activeBorder": "#ff85a276",
    "tab.activeBorderTop": "#ff85a254",
    "tab.activeBackground": "#ff85a232",
    "titleBar.inactiveBackground": "#000000",
    "editorHoverWidget.background": "#000000",
